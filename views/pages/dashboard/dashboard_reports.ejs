<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href='https://unpkg.com/boxicons@2.0.9/css/boxicons.min.css' rel='stylesheet'>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
	<link rel="stylesheet" href="/assets/google-analytics.css">
     <!-- Material Icons -->
     <link href="https://fonts.googleapis.com/icon?family=Material+Icons+Outlined" rel="stylesheet">
     

	<title>Analytics</title>
</head>
<body>
	<!-- SIDEBAR -->
    <%- include ('dashboard_sidebar') -%>

	<section id="content">
		<nav>
			<i class='bx bx-menu toggle-sidebar' ></i>
			<form action="#">
				<div class="form-group">
				</div>
			</form>
			<a href="#" class="nav-link">
				<i class='bx bxs-bell icon' ></i></a>
			<div class="profile">
				<img src="user.png" alt="">
				<ul class="profile-link">
					<li><a href="#"><i class='bx bxs-user-circle icon' ></i> Account</a></li>
					<li><a href="#"><i class='bx bxs-cog' ></i>Settings</a></li>
					<li><a href="#"><i class='bx bxs-log-out-circle' ></i> Logout</a></li>
			</div>
		</nav>
		<div class="dash-content">
            <div class="overview">
		<!-- Main -->
        <main class="main-container">
            <div class="main-title">
              <h1 >Analytics</h1>
            </div>
            
            <div class="container mt-3">
                <div class="table-responsive">
                  <table class="table table-bordered">
                    <thead>
                      <tr bgcolor="#D3D3D3">
                        <th>Product &emsp; &emsp; &emsp; &emsp; &emsp; &emsp; &emsp; &emsp;</th>
                        <th>Product List Views</th>
                        <th>Product Detail Views</th>
                        <th>Product Adds to Basket</th>
                        <th>Product Removes from Basket</th>
                        <th>Product Checkouts</th>
                        <th>Unique Purchases</th>
                        <th>Basket-to-Detail Rate</th>
                        <th>Buy-to-Detail Rate</th>
                      </tr>
                    </thead>
                    <tbody>
                      <tr bgcolor="#eee">
                        <td></td>
                        <td><%= total[0].sr_sum %><br><p class="text-muted mb-1">% Total: 100.00%</p></td>
                        <td><%= total[0].dr_sum %><br><p class="text-muted mb-1">% Total: 100.00%</p></td>
                        <td><%= total[0].ac_sum %><br><p class="text-muted mb-1">% Total: 100.00%</p></td>
                        <td><%= total[0].rm_sum %><br><p class="text-muted mb-1">% Total: 100.00%</p></td>
                        <td><%= total[0].res_sum %><br><p class="text-muted mb-1">% Total: 100.00%</p></td>
                        <td><%= total[0].uq_sum %><br><p class="text-muted mb-1">% Total: 100.00%</p></td>
                        <td><%= total[0].a2d_percent %><br><p class="text-muted mb-1">% Total: 100.00%</p></td>
                        <td><%= total[0].u2d_percent %><br><p class="text-muted mb-1">% Total: 100.00%</p></td>
                      </tr>
                      <% report.forEach( function(googol){  %>
                          <tr>
                              <td><a href="#"><%= googol.item_name %></a></td>
                              <td><%= googol.search_rate %>   <span style="size:smaller; color: grey;">(<%=googol.sr_percent %>%)</span></td>
                            <td><%= googol.detail_rate %>   <span style="size:smaller; color: grey;">(<%=googol.dr_percent %>%)</span></td>
                            <td><%= googol.add_cart %>   <span style="size:smaller; color: grey;">(<%=googol.ac_percent %>%)</span></td>
                            <td><%= googol.rm_cart %>   <span style="size:smaller; color: grey;">(<%=googol.rm_percent %>%)</span></td>
                            <td><%= googol.reservations %>   <span style="size:smaller; color: grey;">(<%=googol.res_percent %>%)</span></td>
                            <td><%= googol.unique_rental %>   <span style="size:smaller; color: grey;">(<%=googol.ur_percent %>%)</span></td>
                            <td><%= googol.a2d_percent %>%</td>
                            <td><%= googol.u2d_percent %>%</td>
                          </tr>
                      <% }) %>
                    </tbody>
                  </table>
                </div>
              </div>
        </div>
          </main>
          <!-- End Main -->
    
        </div>
    
        <!-- Scripts -->
				
	</section>
	<script src="/assets/scripts/google-analytics.js"></script>
</body>
</html>
